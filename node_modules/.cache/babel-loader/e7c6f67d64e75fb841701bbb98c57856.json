,WAAA,MAAA,CAAA,IAAA;AACA,WAAA,MAAA,CAAA,gBAAA,CAAA,OAAA,EAAA,YAAA;AACA,aAAA,KAAA;;AACA,YAAA,KAAA,KAAA,GAAA,KAAA,KAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,eAAA,KAAA,GAAA,CAAA;AACA;;AACA,aAAA,OAAA,GAAA,KAAA,KAAA,CAAA,KAAA,KAAA,CAAA;AACA,aAAA,IAAA,CAAA,KAAA,OAAA;AACA,OAPA,CAOA,IAPA,CAOA,IAPA,CAAA;AAQA,WAAA,MAAA,GAAA,IAAA;AACA,KAjBA;AAkBA,IAAA,KAlBA,mBAkBA;AACA,WAAA,MAAA,CAAA,KAAA;AACA,WAAA,MAAA,GAAA,KAAA;AACA,KArBA;AAsBA,IAAA,IAtBA,kBAsBA;AACA,WAAA,KAAA;;AACA,UAAA,KAAA,KAAA,GAAA,KAAA,KAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,aAAA,KAAA,GAAA,CAAA;AACA;;AACA,WAAA,OAAA,GAAA,KAAA,KAAA,CAAA,KAAA,KAAA,CAAA;AACA,WAAA,IAAA,CAAA,KAAA,OAAA;AACA,KA7BA;AA8BA,IAAA,IA9BA,kBA8BA;AACA,WAAA,KAAA;;AACA,UAAA,KAAA,KAAA,GAAA,CAAA,EAAA;AACA,aAAA,KAAA,GAAA,KAAA,KAAA,CAAA,MAAA,GAAA,CAAA;AACA;;AACA,WAAA,OAAA,GAAA,KAAA,KAAA,CAAA,KAAA,KAAA,CAAA;AACA,WAAA,IAAA,CAAA,KAAA,OAAA;AACA;AArCA,GArCA;AA4EA,EAAA,OA5EA,qBA4EA;AACA,SAAA,OAAA,GAAA,KAAA,KAAA,CAAA,KAAA,KAAA,CAAA;AACA,SAAA,MAAA,CAAA,GAAA,GAAA,KAAA,OAAA,CAAA,GAAA;AACA,SAAA,MAAA,CAAA,KAAA;AACA,GAhFA;AAiFA,EAAA,QAAA,EAAA,EAjFA;AAoFA,EAAA,UAAA,EAAA;AApFA,CAAA","sourcesContent":["<template>\n  <div id=\"app\">\n    <header>\n      <h1>My Music</h1>\n    </header>\n    <main>\n      <section class=\"player\">\n        <h2 class=\"song-title\">{{ current.artist }} - <span>{{ current.title }}</span></h2>\n        <div class=\"control\">\n          <button class=\"prev\" @click=\"prev\">Prev</button>\n          <button class=\"play\" v-if=\"!isPlay\" @click=\"play\">Play</button>\n          <button class=\"pause\" v-else @click=\"pause\">Pause</button>\n          <button class=\"next\" @click=\"next\">Next</button>\n        </div>\n      </section>\n      <section class=\"playlist\">\n        <h3>The Playlist</h3>\n        <button v-for=\"song in songs\" :key=\"song.src\" @click=\"play(song)\" :class=\"(song.src == current.src) ? 'song playing' : 'song'\">\n          {{ song.title }} - {{ song.artist }}\n        </button>\n      </section>\n    </main>\n  </div>\n</template>\n\n<script>\n\nexport default {\n  name: 'app',\n  data() {\n    return {\n      current: {},\n      index: 0,\n      isPlay: false,\n      songs: [\n        {\n          title: 'Paris',\n          artist: 'The Chainsmokers',\n          src: require('./assets/The_Chainsmokers-Paris.mp3')\n        },\n        {\n          title: 'This Feeling',\n          artist: 'The Chainsmokers',\n          src: require('./assets/The_Chainsmokers-This_Feeling.mp3')\n        },\n        {\n          title: 'Car Radio',\n          artist: 'Twenty One Pilots',\n          src: require('./assets/Twenty_One_Pilots-Car_Radio.mp3')\n        },\n        {\n          title: 'Invincible',\n          artist: 'DEAF KEV',\n          src: require('./assets/DEAF KEV - Invincible [NCS Release].mp3')\n        },\n        {\n          title: 'On & On',\n          artist: 'Cartoon',\n          src: require('./assets/Cartoon - On & On (feat. Daniel Levi) [NCS Release].mp3')\n        },\n      ],\n      player: new Audio()\n    }\n  },\n  methods: {\n  play(song) {\n      if (typeof song.src != \"undefined\") {\n        this.current = song;\n\n        this.player.src = this.current.src;\n      }\n      this.player.play();\n      this.player.addEventListener('ended', function() {\n        this.index++;\n        if(this.index > this.songs.length -1 ) {\n          this.index = 0;\n        }\n        this.current = this.songs[this.index];\n        this.play(this.current);\n      }.bind(this));\n      this.isPlay = true;\n    },\n    pause() {\n      this.player.pause();\n      this.isPlay = false;\n    },\n    next() {\n      this.index++;\n      if(this.index > this.songs.length -1 ) {\n        this.index = 0;\n      }\n      this.current = this.songs[this.index];\n      this.play(this.current);\n    },\n    prev() {\n      this.index--;\n      if(this.index < 0) {\n        this.index = this.songs.length -1;\n      }\n      this.current = this.songs[this.index];\n      this.play(this.current);\n    }\n  },\n  created() {\n    this.current = this.songs[this.index];\n    this.player.src = this.current.src;\n    this.player.pause();\n  },\n  computed: {\n    \n  },\n  components: {\n    \n  }\n}\n</script>\n\n<style>\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n}\nbody {\n  font-family: sans-serif;\n}\nheader {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  padding: 15px;\n  background-color: #212121;\n  color: #FFF;\n}\nmain {\n  width: 100%;\n  max-width: 768px;\n  margin: 0 auto;\n  padding: 25px;\n}\n.song-title {\n  color: #53565a;\n  font-size: 32px;\n  font-weight: 700;\n  text-transform: uppercase;\n  text-align: center;\n}\n.control {\n  display: flex;\n  justify-content: center;\n  padding: 30px 15px;\n}\nbutton {\n  appearance: none;\n  background: none;\n  border: none;\n  outline: none;\n  cursor: pointer;\n}\n.play, .pause{\n  font-size: 30px;\n  font-weight: 700;\n  padding: 15px 25px;\n  margin: 0px 15px;\n}\n.playlist {\n  padding: 0px 30px;\n}\n.playlist h3 {\n  color: #212121;\n  font-size: 28px;\n  font-weight: 400;\n  margin-bottom: 30px;\n  text-align: center;\n}\n.playlist .song {\n  display: block;\n  width: 100%;\n  padding: 15px;\n  font-size: 20px;\n  font-weight: 700;\n  cursor: pointer;\n}\n.playlist .song.playing {\n  color: #FFF;\n  background-image: linear-gradient(to right, rgb(206, 176, 202), rgb(255, 4, 234))\n}\n</style>\n"],"sourceRoot":"src"}]}